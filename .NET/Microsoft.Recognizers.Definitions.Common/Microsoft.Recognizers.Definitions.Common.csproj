<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net462;net452;net45</TargetFrameworks>
	<PackageId>Microsoft.Recognizers.Text.Definitions.Common</PackageId>
    <Authors />
    <Company>Microsoft</Company>
    <Product>Microsoft.Recognizers.Text.Definitions.Common</Product>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="xcopy /E &quot;$(TargetDir)*&quot; &quot;$(TargetDir)..\..\..\..\build\package\&quot; /Y" />
  </Target>

  <ItemGroup>
    <PackageReference Include="YamlDotNet" Version="4.2.1" />
  </ItemGroup>

</Project>
